{
  "name": "wallaby",
  "version": "1.0.0",
  "description": "a robust electron client for Ollama and other generic llm applications",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "dev:renderer": "vite --config vite.renderer.config.js",
    "build:renderer": "vite build --config vite.renderer.config.js",
    "build:preload": "vite build --config vite.preload.config.js",
    "build": "npm run build:preload && npm run build:renderer",
    "start": "npm run build && electron ."
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  },
  "keywords": [
    "Electron"
  ],
  "author": "kmalone",
  "license": "MIT",
  "dependencies": {
    "dompurify": "^3.3.1",
    "electron-store": "^11.0.2",
    "marked": "^17.0.2"
  },
  "devDependencies": {
    "vite": "^7.3.1"
  }
}
